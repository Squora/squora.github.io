# Application Configuration
# Copy this file to .env and fill in the values

# Site URL (used for canonical URLs and Open Graph)
VITE_SITE_URL=https://squora.github.io

# EmailJS Configuration (for contact form)
# Get these values from https://dashboard.emailjs.com/
VITE_EMAILJS_SERVICE_ID=your_service_id
VITE_EMAILJS_TEMPLATE_ID=your_template_id
VITE_EMAILJS_PUBLIC_KEY=your_public_key

# Telegram Bot Configuration (for contact form notifications)
# Create bot via @BotFather, get chat_id via @userinfobot
VITE_TELEGRAM_BOT_TOKEN=your_bot_token
VITE_TELEGRAM_CHAT_ID=your_chat_id

# Analytics (optional)
VITE_ANALYTICS_ID=

# Feature Flags
VITE_ENABLE_CONTACT_FORM=true
